package tp1;

/**
 * @author Florence
 *
 */
public class IP {
	
	private int oct1;
	private int oct2;
	private int oct3;
	private int oct4;
	
	/**
	 * Constructeur 
	 * @param oct1
	 * @param oct2
	 * @param oct3
	 * @param oct4
	 */
	public IP (int oct1, int oct2, int oct3, int oct4){
		this.oct1 = oct1;
		this.oct2 = oct2;
		this.oct3 = oct3;
		this.oct4 = oct4;
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		IP IPv4 = new IP(Integer.parseInt(args[0]), Integer.parseInt(args[1]), Integer.parseInt(args[2]), Integer.parseInt(args[3]));
	}

}
